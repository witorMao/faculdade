
public class NoLista <K>{
	private K objeto;
	private NoLista prox;
	
	
	@SuppressWarnings("unchecked")
	public NoLista(K ob) {
		this.objeto = (K) new Object();
		this.objeto = ob;
		this.prox = null;
	}
	
	public K getNoLista() {
		return this.objeto;
	}
	
	public void setNoLista(K o) {
		this.objeto = o;
	}
	
	public NoLista <K> getNoListaProx() {
		return (K)this.prox;
	}
	
	public void setNoListaProx(NoLista p) {
		this.prox = p;
	}
}


