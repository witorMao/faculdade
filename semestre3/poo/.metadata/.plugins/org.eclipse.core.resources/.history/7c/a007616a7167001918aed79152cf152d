
public class NoLista <K>{
	private K objeto;
	private NoLista <K> prox;
	
	
	@SuppressWarnings("unchecked")
	public NoLista(K ob) {
		this.objeto = (K) new Object();
		this.objeto = ob;
		this.prox = null;
	}
	
	public K getNoLista() {
		return this.objeto;
	}
	
	public void setNoLista(K o) {
		this.objeto = o;
	}
	
	public NoLista <K>getNoListaProx() {
		return this.prox;
	}
	
	public void setNoListaProx(NoLista <K> p) {
		this.prox = p;
	}
	
	public void printaNoLista() {
		System.out.println(this.objeto);
	}
}


