
public class NoLista <K>{
	private K objeto;
	private NoLista prox;
	
	public NoLista(K ob) {
		this.objeto = ob;
		this.prox = null;
	}
	
	public K getNoLista() {
		return this.objeto;
	}
	
	public void setNoLista(K o) {
		this.objeto = o;
	}
	
	public Nolista getNoListaProx() {
		return this.prox;
	}
	
	public void setNoListaProx(NoLista p) {
		this.prox = p;
	}
}


