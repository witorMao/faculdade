import java.io.IOException;

public class exercicio5 {
	public static void main(String[] args) throws IOException {
		int x = EntradaTeclado.leInt();
		double resto;
		int i;
		int j = x-1;
		while(j != 0){
			for(i = 1; i <= j; ++i){
				resto = j%i;
				if(resto == 0 && (i != 1 && i != j)){//não é primo
					break;
				}
			}
			if(i == j){
				System.out.printf("%d é o primo mais próximo de %d", j, x);
			}	
			--j;
		}	
	}
}
