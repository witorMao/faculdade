import java.io.IOException;

public class exercicio4 {
	public static void main(String[] args) throws IOException {
		int x = EntradaTeclado.leInt();
		double resto;
		for(int i = 1; i <= x; ++i ){
			resto = x%i;
			if(resto == 0 && (i == 1 || i == x)) {
				System.out.printf("É primo"\n);
				break;
			}
		}
		
	}
}
